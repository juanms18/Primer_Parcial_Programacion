#ifndef ARRAY_TRACKING_H_
#define ARRAY_TRACKING_H_

typedef struct{
	int idTracking;
	int idPoducto;
	short int isEmpty;

	int cantidad;
	int distanciaKM;
	long int horaLlegada;
}Tracking;

