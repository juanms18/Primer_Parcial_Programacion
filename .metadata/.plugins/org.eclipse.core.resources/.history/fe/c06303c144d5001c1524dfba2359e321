#include <stdio.h>
#include <stdlib.h>
#include <limits.h>
#include <string.h>

#ifndef ARRAY_TRACKING_H_
#define ARRAY_TRACKING_H_
#define FULL 1
#define EMPTY 0
#define BAJA 2

typedef struct{
	int idTracking;
	int idPoducto;
	short int isEmpty;
	int idProducto;
	int cantidad;
	int distanciaKM;
	long int horaLlegada;
}Tracking;

#endif /* ARRAY_TRACKING_H_ */
