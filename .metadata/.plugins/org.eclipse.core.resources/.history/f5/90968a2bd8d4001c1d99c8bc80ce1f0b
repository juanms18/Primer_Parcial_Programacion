#ifndef ARRAY_USUARIO_H_
#define ARRAY_USUARIO_H_
#include "Input.h"
#define ADMIN 1
#define USUARIO 2
#define FULL 1
#define EMPTY 0
#define BAJA 2



typedef struct{
	int idUsuario;
	short int isEmpty;
	short int tipo;
	char correo [40];
	char direccion[50];
	char password[25];
	int codPostal;
}Usuario;

int ingresarComoUsuarioOAdmin(Usuario * eUsuario , int len);

int buscarCorreoExistenteYPassCorrecta(Usuario * eUsuario , int len , int* indice, int*tipo);

int initUsuario( Usuario* Usuarios ,int len);

int BuscarPrimerEspacioLibre ( Usuario* listaUsuarios ,int len);

static int obtenerID(void);;

#endif /* ARRAY_USUARIO_H_ */
