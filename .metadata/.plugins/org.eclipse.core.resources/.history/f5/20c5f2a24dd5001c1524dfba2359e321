#include "Array_Tracking.h"



int initTracking( Tracking *  lista,int len)
{
   	int retorno = -1;
    int i;

   	if(lista != NULL && len > 0)
   	{
   	 retorno=0;
	 for ( i = 0; i < len; i++)
	    {
	     lista[i].isEmpty = EMPTY;
	    }
   	}
	return retorno;
}


int BuscarPrimerEspacioLibreTracking (Tracking * list,int len)
{
	  int i;
	  int retorno;

	  retorno = -1;

	  if(list != NULL && len > 0)
	  {
	  for (i = 0; i < len; i++)
	    {
		 if (list[i].isEmpty == EMPTY)
		{
		  retorno = i;
		  break;
		}
	    }
	  }
	  return retorno;
}

